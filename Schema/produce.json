{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://raw.github.com/Charles1625/crop-production-ontology/Schema/produce.json",

	"description": "Description of plant material harvested for some use",
	"type": "object",
	"properties": {
		"species": {
			"description": "Species of the plants",
			"type": "string",
			"enum": ["agreed list of species names goes here"]
		},
		"variety": {
			"description": "Variety (cultivar) of the plants",
			"type": "string",
			"enum": ["agreed list of variety names goes here"]
		},
		"part": {
			"description": "part of the plants",
			"type": "string",
			"enum": ["agreed list of crop part names goes here"]
		}
	},
	"required": [ "species", "part" ]
}